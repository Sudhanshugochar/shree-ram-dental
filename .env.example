# Copy this file to .env.local and fill in your Google Service Account credentials
# DO NOT commit .env.local to git - it contains sensitive information

# Your Google Service Account Key (JSON format as a single line)
# Get this from Google Cloud Console → Service Accounts → Keys → Create JSON key
GOOGLE_SERVICE_ACCOUNT_KEY='{"type":"service_account","project_id":"your-project-id","private_key_id":"your-private-key-id","private_key":"-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----\n","client_email":"your-app@your-project.iam.gserviceaccount.com","client_id":"your-client-id","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/your-app%40your-project.iam.gserviceaccount.com"}'
